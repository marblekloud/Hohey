ption,\n      creator: filmCreator,\n    });\n    Array.from(document.querySelectorAll(\"input\")).forEach(\n      input => (input.value = \"\")\n    );\n  }\n\n  return (\n    <div className=\"App\">\n      <div className=\"App-header\">\n        <h1>Film Name</h1>\n        <Input \n          type=\"text\" \n          onChange={(e) => {\n            setFilmName(e.target.value);\n          }}\n        />\n        <h1>Film Creator</h1>\n        <Input \n          type=\"text\" \n          onChange={(e) => {\n            setFilmCreator(e.target.value);\n          }}\n        />\n        <h1>Film Description</h1>\n        <Input \n          type=\"text\" \n          onChange={(e) => {\n            setFilmDescription(e.target.value);\n          }}\n        />\n        <h1></h1>\n        <Button\n          color=\"green\"\n          onClick={() => addData()}\n        >Publish Video</Button>\n      </div>\n    </div>\n  );\n}\n\nexport default Uploads;\n","/Users/kelvinho/Documents/Coding/HoHey-main 4 2/hoheywebsite/client/src/Home.tsx",["61","62","63","64"],"import React from \"react\";\nimport { useState } from \"react\";\nimport useAsync from \"./components/useAsync\";\nimport { Input, Message, Button } from \"semantic-ui-react\";\nimport { unlockAccount } from \"./api/web3\";\nimport { useWeb3Context } from \"./contexts/Web3\";\nimport 'semantic-ui-css/semantic.min.css';\nimport Axios from 'axios';\nimport './App.css';\n\nfunction Home() {\n\n  const {\n    state: { account },\n    updateAccount\n  } = useWeb3Context();\n  \n  const { pending, error, call } = useAsync(unlockAccount);\n\n  async function onClickConnect() {\n    const { error, data } = await call(null);\n\n    if (error) {\n      console.error(error);\n    }\n    if (data) {\n      updateAccount(data)\n    }\n  }\n\n  return (\n    <div className=\"App\">\n      <div className=\"App-header\">\n        <h1>Metamask</h1>\n        <div>Account: {account}</div>\n        <Message warning>Metamask is not connected</Message>\n        <Button \n          color=\"blue\"\n          onClick={() => onClickConnect()}\n          disabled={pending}\n          loading={pending}\n        >Connect to Metamask</Button>\n      </div>\n    </div>\n  );\n}\n\nexport default Home;\n","/Users/kelvinho/Documents/Coding/HoHey-main 4 2/hoheywebsite/client/src/contexts/Web3.tsx",[],"/Users/kelvinho/Documents/Coding/HoHey-main 4 2/hoheywebsite/client/src/api/web3.ts",[],"/Users/kelvinho/Documents/Coding/HoHey-main 4 2/hoheywebsite/client/src/components/useAsync.tsx",[],"/Users/kelvinho/Documents/Coding/HoHey-main 4 2/hoheywebsite/client/src/App.tsx",[],"/Users/kelvinho/Documents/Coding/HoHey-main 4 2/hoheywebsite/client/src/components/UploadPage.tsx",["65"],"import React, { PureComponent } from 'react';\nimport './App.css';\nimport axios from 'axios';\nimport Upload from '../assets/images/upload.png';\nimport styles from './mystyle.module.css'; \n\ninterface UploadState {\n    recentImage: any;\n    caption: string;\n    uploadedImageUrl: string;\n    uploadedImage: any;\n};\n\nclass UploadPage extends PureComponent<{}, UploadState> {\n    constructor(props: {}) {\n        super(props);\n\n        this.state = {\n            recentImage: {},\n            caption: '',\n            uploadedImageUrl: '',\n            uploadedImage: {},\n        };\n    }\n\n    componentDidMount = () => {\n        this.fetchRecent();\n    }\n\n    fetchRecent = () => {\n        axios.get('http://localhost:9890/recent')\n            .then((response) => {\n                this.setState({ recentImage: response.data.image });\n            })\n            .catch(err => alert('Error: ' + err));\n    }\n\n    uploadImage = () => {\n        if (!this.state.caption.trim() || !this.state.uploadedImage.name) {\n            return alert('Caption or file is missing');\n        }\n\n        let formData = new FormData();\n        formData.append('caption', this.state.caption);\n        formData.append('file', this.state.uploadedImage);\n\n        axios.post('http://localhost:9890/', formData)\n            .then((response) => {\n                response.data.success ? alert('File successfully uploaded') : alert('File already exists');\n                this.fetchRecent();\n            })\n            .catch(err => alert('Error: ' + err));\n    }\n\n    render() {\n        return (\n            <div className={styles.UploadPage}>\n                <div className={styles.Recent}>\n                    <p className={styles.Recent_Title}>Recently uploaded file</p>\n                    <div className={styles.ImageBox}>\n                        <div className=\"CaptionBox\">\n                            <p className=\"ImageBox__Caption\">Caption</p>\n                            <span className=\"ImageBox__CaptionValue\">{this.state.recentImage.caption}</span>\n                        </div>\n\n                        <video src={'http://localhost:9890/image/' + this.state.recentImage.filename}  width = \"320\" height = \"240\" controls>\n                        </video>\n                    </div>\n                </div>\n\n                <div className={styles.Recent}>\n                    <p className=\"Upload__Title\">Upload File</p>\n                    <div className=\"Upload__InputSection\">\n                        <input\n                            type=\"text\"\n                            className=\"Upload__Caption\"\n                            placeholder=\"Enter caption...\"\n                            onChange={event => this.setState({ caption: event.target.value })}\n                            value={this.state.caption}\n                        />\n                        <input\n                            type=\"file\"\n                            className=\"Upload__Input\"\n                            onChange={(event: any) => {\n                                this.setState({\n                                    uploadedImageUrl: URL.createObjectURL(event.target.files[0]),\n                                    uploadedImage: event.target.files[0],\n                                })\n                            }}\n                        />\n                    </div>\n\n                    <img\n                        src={!this.state.uploadedImageUrl.trim() ? Upload : this.state.uploadedImageUrl}\n                        alt=\"upload-image\"\n                        className=\"Upload__Image\"\n                    />\n\n                    <button onClick={this.uploadImage} className=\"Upload__Button\">Upload</button>\n                </div>\n            </div>\n        );\n    }\n}\n\nexport default UploadPage;","/Users/kelvinho/Documents/Coding/HoHey-main 4 2/hoheywebsite/client/src/components/ListPage.tsx",[],{"ruleId":"66","severity":1,"message":"67","line":9,"column":8,"nodeType":"68","messageId":"69","endLine":9,"endColumn":11},{"ruleId":"66","severity":1,"message":"70","line":16,"column":13,"nodeType":"68","messageId":"69","endLine":16,"endColumn":26},{"ruleId":"71","replacedBy":"72"},{"ruleId":"73","replacedBy":"74"},{"ruleId":"66","severity":1,"message":"75","line":3,"column":8,"nodeType":"68","messageId":"69","endLine":3,"endColumn":12},{"ruleId":"66","severity":1,"message":"76","line":4,"column":8,"nodeType":"68","messageId":"69","endLine":4,"endColumn":16},{"ruleId":"66","severity":1,"message":"77","line":5,"column":17,"nodeType":"68","messageId":"69","endLine":5,"endColumn":24},{"ruleId":"66","severity":1,"message":"78","line":6,"column":10,"nodeType":"68","messageId":"69","endLine":6,"endColumn":23},{"ruleId":"66","severity":1,"message":"79","line":7,"column":10,"nodeType":"68","messageId":"69","endLine":7,"endColumn":24},{"ruleId":"80","severity":1,"message":"81","line":53,"column":9,"nodeType":"82","endLine":53,"endColumn":13},{"ruleId":"66","severity":1,"message":"83","line":2,"column":10,"nodeType":"68","messageId":"69","endLine":2,"endColumn":18},{"ruleId":"66","severity":1,"message":"84","line":4,"column":10,"